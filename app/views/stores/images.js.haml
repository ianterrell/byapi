media_urls = {
= @store.designs.where("cafepress_section_id is not null").map{|d|"#{d.cafepress_top_section_id}: '#{d.cafepress_media_url}'"}.join(",\n")
};
jQuery(function($) {
$('img').each(function(index, img){
var src = $(img).attr('src');
var match = src.match(/http:\/\/logo\.cafepress\.com\/(nocache\/)?\d+\/\d+\.(\d+)\.png/);
if (match) {
$(img).attr('src',media_urls[match[2]]);
$(img).attr('height','200');
$(img).attr('width','200');
}
});
});